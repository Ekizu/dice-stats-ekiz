                //stroke: 'black',
                //strokeWidth: 5,
                fill: '#faf0e1',
                fillOpacity: 0.1,
                },
            },
            hAxis: {
                title: 'Die Number',
                titleTextStyle:{
                    fontSize:20,
                },
                textStyle:{
                    fontSize: 12,
                },
                gridlines:{
                    color:'black',
                },
